	"license": "BSD-3-Clause",
	"main": "dist/index.js",
	"name": "role-mention-cooldown",
	"version": "1.1.0",
	"dependencies": {
		"better-console-utilities": "^1.1.0",
		"discord.js": "^14.14.1",
		"dotenv": "^16.3.1",
		"moment": "^2.30.1",
		"mongoose": "^8.8.2"
	},
	"devDependencies": {
		"@types/mongoose": "^5.11.97",
		"@types/node": "^20.10.6",
		"nodemon": "^3.0.2",
		"rimraf": "^5.0.5",
		"tsx": "^4.7.0",
		"typescript": "^5.7.2"
	},
	"scripts": {
